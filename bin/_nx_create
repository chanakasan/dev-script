main() {
  local name="$1"
  if [ -z $name ]; then
    echo " usage: nex create <module>"
    exit 1
  fi
  local full_script_name=create/$name.sh
  local file_path=$devscript_path/src/$full_script_name

  if [ -f $file_path ]; then
    echo " Running script: $file_path"
    bash  $file_path "${@:2}"
  else
    echo " script not found: $file_path"
  fi
}

main "$@"